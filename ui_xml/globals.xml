    <px name="border_radius_sm" value="4"/>

    <!-- ===================================================================== -->
    <!-- Unified Responsive Spacing System (space_*)                         -->
    <!-- ===================================================================== -->
    <!-- Runtime selects appropriate variant via ui_theme_register_responsive_spacing()
         based on max(width, height): SMALL (≤480px), MEDIUM (481-800px), LARGE (>800px)
         Target hardware: 480x320 (SMALL), 800x480 (MEDIUM), 1024x600+ (LARGE)

         CRITICAL: Base constants (space_xxs, space_xs, etc.) are registered at
         runtime by ui_theme.cpp - do NOT define them here or responsive overrides
         will be silently ignored due to LVGL's lv_xml_register_const() behavior. -->
    <!-- space_xxs: Tiny spacing (keypad rows, compact icon gaps) -->
    <px name="space_xxs_small" value="2"/>
    <px name="space_xxs_medium" value="3"/>
    <px name="space_xxs_large" value="4"/>
    <!-- space_xs: Extra small (button icon+text, dense info) -->
