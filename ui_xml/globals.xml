    <!-- nav_width and overlay_panel_width are registered at runtime by
         ui_theme_register_responsive_spacing() based on screen size.
         DO NOT define them here - LVGL ignores duplicate registrations.
         Values: nav_width = 64/76/102px (small/medium/large breakpoints)
                 overlay_panel_width = screen - nav - space_lg gap -->
    <!-- UI dimensions -->
    <px name="nav_padding" value="16"/>
    <px name="nav_button_size" value="70"/>
    <!-- border_radius is responsive: _small/_medium/_large registered as border_radius based on screen size -->
    <px name="border_radius_small" value="4"/>
